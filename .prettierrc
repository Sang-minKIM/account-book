{
  "printWidth": 120,
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^[@/]",
    "^~/providers/(.*)$",
    "^~/pages/(.*)$",
    "^~/components/(.*)$",
    "^~/utils/(.*)$",
    "^~/(.*)$",
    "^[./]",
    "^[../]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}
